programa {
  funcao inicio() {
    inteiro num1, num2, resto, divisao
    escreva("Digite o primeiro número: ")
    leia(num1)
    escreva("Digite o segundo número: ")
    leia(num2) 

    divisao = num1 / num2
    resto = num1 % num2

    escreva("\nA divisão dos números é: ", divisao)
    escreva("\nO Resto dos números é: ", resto)
  }
}
